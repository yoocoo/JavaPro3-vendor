    <!--1.1.1生产商管理员注册售货机 在第三方支付表中 添加记录-->
    <insert id="addVendor" parameterType="cn.wj.domain.ThirdPay">
      <selectKey keyProperty="thirdpayId" resultType="int" order="AFTER">
        SELECT @@IDENTITY AS thirdpay_id
      </selectKey>
      INSERT into  third_pay(thirdpay_key, vendor_id)
      values (#{thirdpayKey,jdbcType=VARCHAR},0);
    </insert>
  <!--1.1.2 功能说明：更新售货机id  依据从 售货机表 拷贝过来   -->
  <update id="updateVendorId" parameterType="cn.wj.domain.ThirdPay">
    UPDATE third_pay SET
    vendor_id = #{vendorId}
    WHERE  thirdpay_key = #{thirdpayKey}
  </update>


